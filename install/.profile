# scaling for HiDPI devices
alias vim=nvim

export SANDBOX="$HOME/sandbox"
export JAVA_HOME="/usr/lib/jvm/default"
export ANDROID_HOME="$HOME/Android/Sdk"
export PATH="$HOME/.bin:$HOME/.local/bin:$ANDROID_HOME/platform-tools:$SANDBOX/flutter/bin:$PATH"
export TERMINAL='termite --title=Term'
export TERM=xterm
export EDITOR=nvim
export VISUAL=nvim
